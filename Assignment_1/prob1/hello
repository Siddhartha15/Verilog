#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55efb1b29d40 .scope module, "t_hello_world" "t_hello_world" 2 1;
 .timescale 0 0;
    .scope S_0x55efb1b29d40;
T_0 ;
    %vpi_call 2 3 "$dumpfile", "hello_world.vcd" {0 0 0};
    %vpi_call 2 4 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55efb1b29d40 {0 0 0};
    %pushi/vec4 100, 0, 32;
T_0.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_0.1, 5;
    %jmp/1 T_0.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 1, 0;
    %vpi_call 2 6 "$monitor", $time, " Hello World" {0 0 0};
    %jmp T_0.0;
T_0.1 ;
    %pop/vec4 1;
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "hello_tb.v";
